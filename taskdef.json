{
    "executionRoleArn": "arn:aws:iam::151268123722:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "nextjs",
        "image": "<IMAGE1_NAME>",
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "tcp"
          }
        ],
        "essential": true
      }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "nextjs-docker"
  }

